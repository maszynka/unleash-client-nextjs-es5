{
  "extends": "tsconfig/react-library.json",
  "compilerOptions": {
    "outDir": ".",
    "rootDir": ".",
    "alwaysStrict": false,
    "noImplicitUseStrict": true,
    "ignoreDeprecations": "5.0",
    "paths": {
      "baseUrl": ["."],
      "./src/*": ["./dist/*"]
    },
    "target": "ES5"
  },
  
  "include": ["client.ts"]
}
